{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "packages/vite/src/*"
      ],
      "@modules/*": [
        "packages/modules/*"
      ],
      "@config/*": [
        "packages/config/*"
      ],
      "@shared/*": [
        "packages/shared/*"
      ],
      "@errors/*": [
        "packages/errors/*"
      ],
      "@utils/*": [
        "packages/utils/*"
      ]
    }
  }
}