{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "packages/vite/src/*"
      ],
      "@modules/*": [
        "packages/electron/modules/*"
      ],
      "@config/*": [
        "packages/electron/config/*"
      ],
      "@shared/*": [
        "packages/electron/shared/*"
      ],
      "@errors/*": [
        "packages/electron/errors/*"
      ],
      "@utils/*": [
        "packages/electron/utils/*"
      ],
      "@preload/*": [
        "packages/preload/*"
      ]
    }
  }
}